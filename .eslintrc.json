{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "object-curly-newline": "off",
    "no-unused-vars": "warn",
    "no-restricted-syntax": "off",
    "consistent-return": "off",
    "operator-linebreak": "off",
    "implicit-arrow-linebreak": "off",
    "no-param-reassign": "off"
  }
}
